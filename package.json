{
  "name": "personal-website",
  "version": "0.0.0",
  "private": true,
  "description": "My personal website.",
  "homepage": "https://github.com/evelynhathaway/personal-website#readme",
  "bugs": {
    "url": "https://github.com/evelynhathaway/personal-website/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evelynhathaway/personal-website.git"
  },
  "license": "MIT",
  "author": "Evelyn Hathaway <npm@evelyn.dev> (https://evelyn.dev)",
  "scripts": {
    "build": "gatsby build",
    "build-beta": "cross-env BUILD_ENV=beta npm run build",
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "lint": "eslint ./",
    "preload-fonts": "gatsby-preload-fonts",
    "serve": "gatsby serve",
    "start": "gatsby build && gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": "./node_modules/conventional-changelog-evelyn/commitlint.config.js"
  },
  "lint-staged": {
    "**/package.json": "sort-package-json",
    "./": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "dependencies": {
    "a11y-react-emoji": "^1.1.2",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.2",
    "gatsby": "^2.24.23",
    "gatsby-image": "^2.4.14",
    "gatsby-plugin-manifest": "^2.4.21",
    "gatsby-plugin-preload-fonts": "^1.2.19",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.24",
    "gatsby-plugin-typography": "^2.5.10",
    "gatsby-source-filesystem": "^2.3.23",
    "gatsby-transformer-sharp": "^2.5.12",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-typography": "^0.16.19",
    "sass": "^1.26.10",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@commitlint/prompt": "^9.1.1",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/typography": "^0.16.3",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-preset-gatsby": "^0.5.4",
    "commitlint": "^9.1.0",
    "conventional-changelog-evelyn": "^1.2.0",
    "eslint": "^7.6.0",
    "eslint-plugin-evelyn": "^3.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-unicorn": "^21.0.0",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-scss-typescript": "^4.0.13",
    "gatsby-plugin-typescript": "^2.4.16",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "sort-package-json": "^1.44.0",
    "typescript": "^3.9.7",
    "worker-loader": "^3.0.0"
  },
  "engines": {
    "node": "^10.0.0"
  }
}
